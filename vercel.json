{
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "(.*)",
      "dest": "/src/$1"
    }
  ]
}
